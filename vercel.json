{
    "routes": [
        {
            "src": "/arc-sw.js",
            "dest": "https://arc.io/arc-sw.js"
        },
	{
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "DENY"
        },
        {
          "key" : "X-XSS-Protection",
          "value" : "1; mode=block"
        }
      ]
    }
    ]
	
}
